{
  "name": "SCIZ",
  "version": "1.0.0",
  "main": "index.html",
  "license": "MIT",
  "sitemap": {
    "siteURL": "https://www.sciz.fr/"
  },
  "scripts": {
    "dev": "parcel serve index.html -d dist",
    "pre-build": "mkdir -p ../dist-public/template && mkdir -p ../dist-public/static",
    "build": "yarn clean && yarn pre-build && parcel build index.html -d ../dist-public/ --public-url /static && yarn post-build",
    "post-build": "mv ../dist-public/index.html ../dist-public/template/ && mv ../dist-public/*.* ../dist-public/static/ && cp src/assets/images/*.* ../dist-public/static/",
    "clean": "rm -rf dist ../dist-public",
    "dist-clean": "yarn clean && rm -rf node_modules .cache"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "2.8.0",
    "chartjs-plugin-datalabels": "^0.6.0",
    "chartjs-plugin-zoom": "^0.7.3",
    "material-design-icons-iconfont": "^5.0.1",
    "tiptap": "^1.25.0",
    "tiptap-extensions": "^1.27.0",
    "v-click-outside": "^2.1.3",
    "v-img-fallback": "^0.1.4",
    "vue": "^2.6.10",
    "vue-clipboard2": "^0.3.0",
    "vue-cookies": "^1.5.13",
    "vue-hot-reload-api": "^2.3.3",
    "vue-lodash": "^2.0.2",
    "vue-moment": "^4.0.0",
    "vue-native-notification": "^1.0.5",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.2",
    "vuetify": "<2.0.0",
    "vuex": "^3.1.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.22.10",
    "style-loader": "^1.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
