{
  "name": "SCIZ",
  "version": "1.0.0",
  "main": "index.html",
  "license": "MIT",
  "sitemap": {
    "siteURL": "https://www.sciz.fr/"
  },
  "scripts": {
    "dev": "parcel serve index.html -d dist",
    "pre-build": "mkdir -p ../dist-public/template && mkdir -p ../dist-public/static",
    "build": "yarn clean && yarn pre-build && parcel build index.html -d ../dist-public/ --public-url /static && yarn post-build",
    "post-build": "mv ../dist-public/index.html ../dist-public/template/ && mv ../dist-public/*.* ../dist-public/static/",
    "clean": "rm -rf dist ../dist-public",
    "dist-clean": "yarn clean && rm -rf node_modules .cache"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.1.6",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "2.7.x",
    "chartjs-plugin-datalabels": "^0.5.0",
    "chartjs-plugin-zoom": "^0.6.6",
    "material-design-icons-iconfont": "^4.0.2",
    "tiptap": "^1.4.0",
    "tiptap-extensions": "^1.3.0",
    "v-click-outside": "^2.0.2",
    "v-img-fallback": "^0.1.4",
    "vue": "^2.5.17",
    "vue-chartjs": "^3.4.0",
    "vue-clipboard2": "^0.2.1",
    "vue-cookies": "^1.5.7",
    "vue-hot-reload-api": "^2.3.1",
    "vue-lodash": "^2.0.0",
    "vue-moment": "^4.0.0",
    "vue-native-notification": "^1.0.5",
    "vue-recaptcha": "^1.1.1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.7",
    "vuex": "^3.0.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.10.1",
    "parcel-plugin-robot": "^1.0.0",
    "parcel-plugin-sitemap": "^1.0.0",
    "sass": "^1.14.3",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.5.17"
  }
}
